<div class="container my-5">
    <h2>Edit Configuration</h2>
    <hr>
    <form action="/updatesetting" method="POST">
        <div class="form-group">
            <label for="value">ITBIS</label>
            <input type="number" step="0.01" class="form-control" id="value" name="value" value="{{value}}" required>
        </div>
        <div class="mt-3">
            <a href="/setting" class="btn btn-secondary">Back</a>
            <button type="submit" class="btn btn-primary">Save</button>
        </div>
    </form>
</div>
